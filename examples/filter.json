{
  "sources": {
    "salaries": {
      "schema": [
        {
          "column_name": "first_name",
          "data_type": "String",
          "mandatory": true
        },
        {
          "column_name": "salary",
          "data_type": "Decimal",
          "mandatory": true
        }
      ],
      "source": {
        "type": "FILE",
        "format": "CSV",
        "path": "./examples/salaries.csv"
      }
    }
  },
  "transformations": {
    "filtered": {
      "sources": [
        "salaries"
      ],
      "operations": [
        {
          "type": "Filter",
          "predicate": "salary > 900.00"
        },
        {
          "type": "Filter",
          "predicate": "first_name != Dave"
        }
      ]
    }
  }
}
